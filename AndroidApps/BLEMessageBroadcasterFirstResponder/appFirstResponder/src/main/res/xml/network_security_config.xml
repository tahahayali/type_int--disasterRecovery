<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!-- Allow cleartext (HTTP) traffic to fennecs.duckdns.org -->
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">fennecs.duckdns.org</domain>
    </domain-config>
</network-security-config>


